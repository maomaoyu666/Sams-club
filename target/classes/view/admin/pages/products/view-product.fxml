<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.collections.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="750.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="controller.admin.pages.stores.ViewStoreController">
    <BorderPane layoutX="14.0" layoutY="14.0">
        <center>
            <GridPane fx:id="formViewStore" alignment="CENTER_RIGHT" prefHeight="332.0" prefWidth="712.0">
                <columnConstraints>
                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="350.0" minWidth="10.0" prefWidth="148.0"/>
                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="603.0" minWidth="10.0" prefWidth="279.0"/>
                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="603.0" minWidth="10.0" prefWidth="140.0"/>
                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="603.0" minWidth="10.0" prefWidth="146.0"/>
                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="603.0" minWidth="10.0" prefWidth="215.0"/>
                </columnConstraints>
                <rowConstraints>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                </rowConstraints>
                <Label text="Store Name"/>
                <TextField fx:id="fieldViewStoreName" GridPane.columnIndex="1"/>
                <Label text="Store Location" GridPane.columnIndex="3"/>
                <TextField fx:id="fieldViewStoreLocation" GridPane.columnIndex="4"/>
                <Label text="Contact Info" GridPane.rowIndex="2"/>
                <TextField fx:id="fieldViewStoreContactInfo" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
                <Label text="Opening Date" GridPane.columnIndex="3" GridPane.rowIndex="2"/>
                <TextField fx:id="fieldViewStoreOpeningDate" GridPane.columnIndex="4" GridPane.rowIndex="2"/>
            </GridPane>
        </center>
        <top>
            <Text fx:id="viewStoreName" strokeType="OUTSIDE" strokeWidth="0.0" text="View store data" BorderPane.alignment="CENTER_LEFT">
                <font>
                    <Font size="24.0"/>
                </font>
            </Text>
        </top>
    </BorderPane>
</AnchorPane>
